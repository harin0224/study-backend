USE scott;

DROP TABLE MEMBER;
CREATE TABLE MEMBER(
	ID INT AUTO_INCREMENT PRIMARY KEY,
	pw varchar(10),
	name varchar(10),
	tel varchar(10),
	create_date timestamp

)