
USE scott;

DROP TABLE MEMBER ;
CREATE TABLE member(
	no INT AUTO_INCREMENT PRIMARY KEY,
	id varchar(10) UNIQUE,
	pw varchar(10),
	name varchar(10),
	tel varchar(10),
	create_date timestamp DEFAULT CURRENT_TIMESTAMP
);

