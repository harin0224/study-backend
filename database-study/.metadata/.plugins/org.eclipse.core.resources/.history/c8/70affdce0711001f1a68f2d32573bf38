CREATE TABLE PCMEMBER
(	ID VARCHAR2(100) NOT NULL,
	PW VARCHAR2(10) NOT NULL,
	NAME VARCHAR2(100) NOT NULL,
	NUM VARCHAR2(100) NOT NULL,
	TIME INT
);

DELETE TABLE PCMEMBER;
